generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Video {
  id       Int     @id @default(autoincrement())
  videoId  String? @db.VarChar(255)
  chapters Json?
  subtitle Json?
}
